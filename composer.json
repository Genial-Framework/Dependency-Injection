{
    "name": "genial/di",
    "type": "library",
    "description": "Dependency-Injection dependent from Genial Framework",
    "homepage": "https://github.com/Genial-Framework/Dependency-Injection",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Genial\\Dependency_Injection\\": "src/"
        },
        "files": ["ar.predefined.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
